import java.util.Scanner;
public class Number{
 public static void main(String[]args){
  Scanner sc= new Scanner(Ststrm.in);
  System.out.println("Enter a number");
  int n=sc.nextInt();
  int original=n;
  int rev=0;
  for(n!=0; n=n/10){
   int digit=n%10;
   rev=rev*10+digit;
  }
  if(original==rev){
   System.out.println("Palindrome");
  }
  else{
   System.out.println("Not a Palindrome");
  }
 }
}
