import java.util.Scanner;
public class 
 public static void main(String[]args){
  Scanner sc=new Scanner(System.in);
  System.out.println("Enter size of array:");
  int size=sc.nextInt();
  int[]array=new int[size];
  int i=0;
  System.out.println("Array elements are:");
  while(i<size){
   array[i]=sc.nextInt();
   i=i+1;
  }
 }
}
